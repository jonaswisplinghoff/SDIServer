extends layout

block content
  div.jumbotron
    h1 #{title}
    p Welcome to #{title}
    p You have #{logs.length} Logs
  table
    tr
      td LogId
      td CallId
      td Timestamp
      td Event
      td Choice
    each log in logs
      tr
        td #{log.id}
        td #{log.callId}
        td #{log.timestamp}
        td #{log.event}
        td #{log.choice}